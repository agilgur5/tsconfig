  "files": [
    "src/"
  ],
  "author": "Anton Gilgur",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/agilgur5/tsconfig",
  "repository": {
    "type": "git",
    "url": "https://github.com/agilgur5/tsconfig.git"
  },
  "bugs": {
    "url": "https://github.com/agilgur5/tsconfig/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/agilgur5"
  },
  "keywords": [
    "typescript",
    "ts",
    "tsconfig",
    "strict",
    "strictest",
    "library",
    "package",
    "base",
    "boilerplate",
    "template"
